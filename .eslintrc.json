{
  "extends": [
    "eslint:recommended",
    "plugin:react-hooks/recommended",
    "plugin:storybook/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "jsxPragma": null
  },
  "env": {
    "browser": true,
    "es2020": true
  }
}
